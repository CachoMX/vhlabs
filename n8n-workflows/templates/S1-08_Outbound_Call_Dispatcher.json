      "id": "a1b2c3d4-0001-4000-8000-000000000004",
      "name": "Count Today's Dispatches",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        -320,
        260
      ],
      "alwaysOutputData": true
    },
    {
      "parameters": {
        "jsCode": "const config = $('Config').item.json;\nconst items = $input.all();\n\n// Count valid dispatches (filter out empty placeholder items)\nconst callsToday = items.filter(item => item.json && item.json.ghl_id).length;\n\n// Outside business hours → remaining = 0 → stops\nif (!config.is_business_hours) {\n  return {\n    json: {\n      remaining: 0,\n      batch_size: 0,\n      calls_today: callsToday,\n      daily_limit: config.daily_limit,\n      reason: `Outside business hours (9am-6pm CT Mon-Fri). Current CT hour: ${config.ct_hour}`\n    }\n  };\n}\n\n// Calculate remaining capacity\nconst remaining = Math.max(0, config.daily_limit - callsToday);\nconst batchSize = Math.min(config.batch_size, remaining);\n\nreturn {\n  json: {\n    remaining: remaining,\n    batch_size: batchSize,\n    calls_today: callsToday,\n    daily_limit: config.daily_limit,\n    reason: remaining > 0 ? `OK - ${remaining} calls remaining today` : `Daily limit reached (${config.daily_limit})`\n  }\n};"
      },
