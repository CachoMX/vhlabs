    {
      "parameters": {
        "jsCode": "const response = $input.first().json;\n  const contentId = $('Extract Transcript from S2-03').first().json.content_id;\n \n  try {\n    const text = response.content[0].text;\n\n    let jsonStr = text;\n    if (text.includes('```json')) {\n      jsonStr = text.split('```json')[1].split('```')[0];\n    } else if (text.includes('```')) {\n      jsonStr = text.split('```')[1].split('```')[0];\n    }\n\n    const parsed = JSON.parse(jsonStr.trim());\n\n    let score = 50;\n    score += Math.min((parsed.hooks?.length || 0) * 5, 25);\n    score += Math.min((parsed.clips?.length || 0) * 5, 15);\n    if (parsed.metadata?.content_type === 'case_study') score += 10;\n\n    const priority = score > 80 ? 'high' : score > 50 ? 'medium' : 'low';\n\n    return {\n      json: {\n        content_id: contentId,\n        hooks: parsed.hooks || [],\n        clips: parsed.clips || [],\n        transcript_structured: parsed.transcript || {},\n        metadata: parsed.metadata || {},\n        score: score,\n        priority: priority,\n        raw_response: text,\n        success: true\n      }\n    };\n  } catch (error) {\n    return {\n      json: {\n        content_id: contentId,\n        success: false,\n        error: error.message,\n        raw_response: response.content?.[0]?.text\n      }\n    };\n  }"
      },
