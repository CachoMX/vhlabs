        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={{ JSON.stringify({\"model\": \"claude-sonnet-4-20250514\", \"max_tokens\": 4096, \"system\": \"You are a JSON-only content extraction agent. You MUST respond with ONLY valid JSON, no text, no markdown,    no explanations. If you cannot extract data, return an empty JSON structure.\", \"messages\": [{\"role\": \"user\", \"content\": $('Get Content Agent Prompt').first().json[0]?.content + '\\n\\nTranscript:\\n'   + $('Extract Transcript from S2-03').first().json.transcript}]}) }} ",
        "options": {
          "timeout": 120000
        }
